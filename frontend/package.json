    "@tiptap/starter-kit": "^2.11.5",
    "axios": "^1.7.9",
    "lowlight": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.29.0",
